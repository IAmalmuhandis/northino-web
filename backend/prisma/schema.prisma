generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model built_projects {
  id                  Int    @id @default(autoincrement())
  project_name        String @db.VarChar(255)
  project_description String @db.Text
  team                String @db.VarChar(255)
}

model client_projects {
  id                  Int    @id @default(autoincrement())
  client_name         String @db.VarChar(255)
  project_name        String @db.VarChar(255)
  project_description String @db.Text
}

model customersmessages {
  id            Int     @id @default(autoincrement())
  email_address String  @db.VarChar(45)
  message       String  @db.LongText
  is_replied    Int     @default(0)
  message_id    String? @db.VarChar(255)
  phone_number  String  @db.VarChar(45)
}

model mentors {
  id                Int     @id @default(autoincrement())
  name              String  @db.VarChar(255)
  fieldOfExperience String  @db.VarChar(255)
  about             String? @db.VarChar(255)
  picture           String? @default("mentor.jpg") @db.VarChar(255)
  uniqueId          String? @db.VarChar(225)
}

model northino_tv {
  id                Int     @id @default(autoincrement())
  section_type      String? @db.VarChar(45)
  video_url         String? @db.VarChar(255)
  video_title       String? @db.VarChar(45)
  video_description String? @db.VarChar(255)
}

model request_service_development {
  id                   Int    @id @default(autoincrement())
  fullname             String @db.VarChar(255)
  email                String @db.VarChar(255)
  phone_number         String @db.VarChar(255)
  business_name        String @db.VarChar(255)
  business_type        String @db.VarChar(255)
  business_description String @db.Text
}

model request_service_mentor {
  id              Int    @id @default(autoincrement())
  fullname        String @db.VarChar(255)
  email           String @db.VarChar(255)
  phone_number    String @db.VarChar(255)
  mentorship_type String @db.VarChar(255)
  experience      String @db.VarChar(255)
}

model request_service_promotion {
  id                   Int    @id @default(autoincrement())
  fullname             String @db.VarChar(255)
  email                String @db.VarChar(255)
  phone_number         String @db.VarChar(255)
  business_name        String @db.VarChar(255)
  business_type        String @db.VarChar(255)
  business_description String @db.Text
}

model subscribers {
  id            Int    @id @default(autoincrement())
  full_name     String @db.VarChar(255)
  email_address String @db.VarChar(45)
}

model bank_details {
  id             Int    @id @default(autoincrement())
  account_name   String @db.VarChar(255)
  account_number String @db.VarChar(255)
  bank_name      String @db.VarChar(255)
}

model course_info {
  id                     Int    @id @default(autoincrement())
  course_title           String @db.VarChar(255)
  course_subtitle        String @db.VarChar(255)
  course_desc            String @db.LongText
  level                  String @db.VarChar(255)
  course_category        String @db.VarChar(255)
  course_image_url       String @db.VarChar(255)
  promotional_video_url  String @db.VarChar(255)
  welcome_message        String @db.LongText
  congratulation_message String @db.LongText
  course_price           Float
}

model images {
  id         Int     @id @default(autoincrement())
  image_url  String? @db.VarChar(100)
  image_name String? @db.VarChar(255)
}

model learner_signup {
  id           Int       @id @default(autoincrement())
  firstname    String    @db.VarChar(255)
  lastname     String    @db.VarChar(255)
  phone_number String    @db.VarChar(255)
  email        String    @db.VarChar(255)
  password     String    @db.VarChar(255)
  timestamp    DateTime? @default(now()) @db.Timestamp(0)
}

model tutor_signup {
  id           Int       @id @default(autoincrement())
  firstname    String    @db.VarChar(255)
  lastname     String    @db.VarChar(255)
  phone_number String    @db.VarChar(255)
  email        String    @db.VarChar(255)
  password     String    @db.VarChar(255)
  experience   String    @db.LongText
  description  String    @db.LongText
  image        String    @db.VarChar(255)
  timestamp    DateTime? @default(now()) @db.Timestamp(0)
}

model tutor_video {
  id            Int    @id @default(autoincrement())
  video_url     String @db.VarChar(255)
  video_desc    String @db.LongText
  video_img_url String @db.VarChar(255)
}

model videos {
  id         Int    @id @default(autoincrement())
  video_url  String @db.VarChar(1000)
  video_name String @db.VarChar(255)
}

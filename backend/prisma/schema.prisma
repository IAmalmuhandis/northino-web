generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model built_projects {
  id                  Int    @id @default(autoincrement())
  project_name        String @db.VarChar(255)
  project_description String @db.Text
  team                String @db.VarChar(255)
}

model client_projects {
  id                  Int    @id @default(autoincrement())
  client_name         String @db.VarChar(255)
  project_name        String @db.VarChar(255)
  project_description String @db.Text
}

model customersmessages {
  id            Int     @id @default(autoincrement())
  email_address String  @db.VarChar(45)
  message       String  @db.LongText
  is_replied    Int     @default(0)
  message_id    String? @db.VarChar(255)
  phone_number  String  @db.VarChar(45)
}

model mentors {
  id                Int     @id @default(autoincrement())
  name              String  @db.VarChar(255)
  fieldOfExperience String  @db.VarChar(255)
  about             String? @db.VarChar(255)
  picture           String? @default("mentor.jpg") @db.VarChar(255)
  uniqueId          String? @db.VarChar(255)
}

model northino_tv {
  id                Int     @id @default(autoincrement())
  section_type      String? @db.VarChar(45)
  video_url         String? @db.VarChar(255)
  video_title       String? @db.VarChar(45)
  video_description String? @db.VarChar(255)
}

model request_service_development {
  id                   Int    @id @default(autoincrement())
  fullname             String @db.VarChar(255)
  email                String @db.VarChar(255)
  phone_number         String @db.VarChar(255)
  business_name        String @db.VarChar(255)
  business_type        String @db.VarChar(255)
  business_description String @db.Text
}

model request_service_mentor {
  id              Int    @id @default(autoincrement())
  fullname        String @db.VarChar(255)
  email           String @db.VarChar(255)
  phone_number    String @db.VarChar(255)
  mentorship_type String @db.VarChar(255)
  experience      String @db.VarChar(255)
}

model request_service_promotion {
  id                   Int    @id @default(autoincrement())
  fullname             String @db.VarChar(255)
  email                String @db.VarChar(255)
  phone_number         String @db.VarChar(255)
  business_name        String @db.VarChar(255)
  business_type        String @db.VarChar(255)
  business_description String @db.Text
}

model subscribers {
  id            Int    @id @default(autoincrement())
  full_name     String @db.VarChar(255)
  email_address String @db.VarChar(45)
}
